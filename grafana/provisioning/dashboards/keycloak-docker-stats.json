        {
          "expr": "max by (container) (docker_container_memory_percent{container=~\"keycloak(|.*)\"})",
          "refId": "A",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
