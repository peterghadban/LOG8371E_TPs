  "panels": [
    {
      "type": "timeseries",
      "title": "CPU — keycloak (%)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "max by (container) (docker_container_cpu_percent{container=~\"keycloak(|.*)\"})",
          "refId": "A",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Memory Used / Limit — keycloak (bytes)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "max by (container) (docker_container_memory_usage_bytes{container=~\"keycloak(|.*)\"})",
          "legendFormat": "Used",
          "refId": "A",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        },
        {
          "expr": "max by (container) (docker_container_memory_limit_bytes{container=~\"keycloak(|.*)\"})",
          "legendFormat": "Limit",
          "refId": "B",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Memory — keycloak (%)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
