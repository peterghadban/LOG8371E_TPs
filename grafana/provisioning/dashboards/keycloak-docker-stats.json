        {
          "expr": "sum by (name) (locust_endpoint_fail_rps)",
          "refId": "A",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
