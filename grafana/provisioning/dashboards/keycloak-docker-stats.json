      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 }
    },
    {
      "type": "timeseries",
      "title": "Requests per second — by endpoint",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 25 },
      "targets": [
        {
          "expr": "sum by (uri) (rate(http_server_requests_seconds_count{uri!~\"/metrics|/health.*\"}[1m]))",
          "refId": "A",
          "instant": false,
          "format": "time_series",
          "interval": "5s"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Error rate (4xx / 5xx) — req/s",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 25 },
