{
  "id": null,
  "title": "Keycloak Endpoint Availability Timeline",
  "tags": ["keycloak", "availability"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
