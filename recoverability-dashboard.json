      "gridPos": { "h": 18, "w": 24, "x": 0, "y": 0 },
      "lines": true,
      "linewidth": 3,
      "nullPointMode": "null",
      "yaxes": [
        {
          "format": "short",
          "min": 0,
          "max": 1,
          "label": "Availability"
        },
        { "format": "short" }
      ],
      "xaxis": {
        "show": true,
        "mode": "time"
      }
    }
  ]
